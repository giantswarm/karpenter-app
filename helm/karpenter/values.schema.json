{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "additionalAnnotations": {
            "type": "object"
        },
        "additionalClusterRoleRules": {
            "type": "array"
        },
        "additionalLabels": {
            "type": "object"
        },
        "affinity": {
            "type": "object",
            "properties": {
                "nodeAffinity": {
                    "type": "object",
                    "properties": {
                        "requiredDuringSchedulingIgnoredDuringExecution": {
                            "type": "object",
                            "properties": {
                                "nodeSelectorTerms": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "matchExpressions": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object",
                                                    "properties": {
                                                        "key": {
                                                            "type": "string"
                                                        },
                                                        "operator": {
                                                            "type": "string"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "podAntiAffinity": {
                    "type": "object",
                    "properties": {
                        "requiredDuringSchedulingIgnoredDuringExecution": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "topologyKey": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "aws": {
            "type": "object",
            "properties": {
                "accountID": {
                    "type": "string"
                }
            }
        },
        "baseDomain": {
            "type": "string"
        },
        "clusterID": {
            "type": "string"
        },
        "controller": {
            "type": "object",
            "properties": {
                "containerName": {
                    "type": "string"
                },
                "env": {
                    "type": "array"
                },
                "envFrom": {
                    "type": "array"
                },
                "extraVolumeMounts": {
                    "type": "array"
                },
                "healthProbe": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        }
                    }
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "digest": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "metrics": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        }
                    }
                },
                "resources": {
                    "type": "object"
                },
                "sidecarContainer": {
                    "type": "array"
                },
                "sidecarVolumeMounts": {
                    "type": "array"
                }
            }
        },
        "dnsConfig": {
            "type": "object"
        },
        "dnsPolicy": {
            "type": "string"
        },
        "extraVolumes": {
            "type": "array"
        },
        "flowSchema": {
            "type": "object",
            "properties": {
                "leaderElection": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "boolean"
                        },
                        "matchingPrecedence": {
                            "type": "integer"
                        }
                    }
                },
                "workload": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "boolean"
                        },
                        "matchingPrecedence": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "global": {
            "type": "object",
            "properties": {
                "podSecurityStandards": {
                    "type": "object",
                    "properties": {
                        "enforced": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "hostNetwork": {
            "type": "boolean"
        },
        "imagePullPolicy": {
            "type": "string"
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "logErrorOutputPaths": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "logLevel": {
            "type": "string"
        },
        "logOutputPaths": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object",
            "properties": {
                "kubernetes.io/os": {
                    "type": "string"
                }
            }
        },
        "podAnnotations": {
            "type": "object"
        },
        "podDisruptionBudget": {
            "type": "object",
            "properties": {
                "maxUnavailable": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "podLabels": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object",
            "properties": {
                "fsGroup": {
                    "type": "integer"
                }
            }
        },
        "priorityClassName": {
            "type": "string"
        },
        "replicas": {
            "type": "integer"
        },
        "revisionHistoryLimit": {
            "type": "integer"
        },
        "schedulerName": {
            "type": "string"
        },
        "service": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "serviceMonitor": {
            "type": "object",
            "properties": {
                "additionalLabels": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "endpointConfig": {
                    "type": "object"
                }
            }
        },
        "settings": {
            "type": "object",
            "properties": {
                "batchIdleDuration": {
                    "type": "string"
                },
                "batchMaxDuration": {
                    "type": "string"
                },
                "clusterCABundle": {
                    "type": "string"
                },
                "clusterEndpoint": {
                    "type": "string"
                },
                "clusterName": {
                    "type": "string"
                },
                "eksControlPlane": {
                    "type": "boolean"
                },
                "featureGates": {
                    "type": "object",
                    "properties": {
                        "nodeRepair": {
                            "type": "boolean"
                        },
                        "spotToSpotConsolidation": {
                            "type": "boolean"
                        }
                    }
                },
                "interruptionQueue": {
                    "type": "string"
                },
                "isolatedVPC": {
                    "type": "boolean"
                },
                "reservedENIs": {
                    "type": "string"
                },
                "vmMemoryOverheadPercent": {
                    "type": "number"
                }
            }
        },
        "strategy": {
            "type": "object",
            "properties": {
                "rollingUpdate": {
                    "type": "object",
                    "properties": {
                        "maxUnavailable": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "terminationGracePeriodSeconds": {
            "type": "null"
        },
        "tolerations": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "key": {
                        "type": "string"
                    },
                    "operator": {
                        "type": "string"
                    }
                }
            }
        },
        "topologySpreadConstraints": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "maxSkew": {
                        "type": "integer"
                    },
                    "topologyKey": {
                        "type": "string"
                    },
                    "whenUnsatisfiable": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
